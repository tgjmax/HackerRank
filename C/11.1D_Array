#include <math.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#define SIZE 10

int main() {

  int arr[SIZE], sum = 0, n;
  int *ptr;
  ptr = (int *)malloc(n * sizeof(int));
  if (ptr == NULL) {
    printf("Error! memory not allocated.");
    exit(0);
  }

  scanf("%d", &n);

  for (int i = 0; i < n; i++) {

    scanf("%d", ptr + i);
  }

  for (int i = 0; i < n; i++) {
    sum += *(ptr + i);
  }
  printf("%d", sum);
  getchar();
  return 0;
}

